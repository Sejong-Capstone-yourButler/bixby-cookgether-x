structure (OrderList) {
  description (현재 주문 내역)
  property (name) {
    type (MenuName)
    min (Optional) max(Many)
  }
  property (align) { // 이거는 주문 내역 말할때 필요한가? 
    type (SortingOption)
    min (Optional) max (One) 
  }
  property (num) {
    type(MenuNum)
    min (Optional) max(Many)
  }
  property (stat) {
    type(Status)
    min (Optional)
  }
}