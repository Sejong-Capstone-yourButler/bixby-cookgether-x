action (ShowWaiting) {
  description (수용할 수 있는 최소인원과 해당메뉴의 남은 수량 화면에 보여준다)
  type(Search)
  collect {
    input (menuname) {
      type (MenuName)
      min (Required)
    }
  }
  output (WaitingList) {
    on-empty{ // 에러처리 수정필요
      halt{
        dialog{
          template ("")
        }
      }
    }
  }
}