action (SpeakStock) {
  description (물어보는 메뉴의 재고를 말해준다.)
  // 품목명 % 말해줘
  type(Search)
  collect {
    input (ingredientName) {
      type (IngredientName)
      min (Required)
    }
    input(request) {
      type(Request)
      min(Required)
    }
  }
  output (StockList) {
    on-empty{ //에러처리 수정필요
      halt{
        dialog{
          template ("")
        }
      }
    }
  }
}