action (ShowStock) {
  description (물어보는 메뉴의 재고를 화면에 보여준다.)
  // 품목명 % 보여줘
  type(Search)
  collect {
    input (ingredientName) {
      type (IngredientName)
      min (Required)
    }
    input (stockKeyword) {
      type (StockKeyword)
      min (Optional)
    }
    input(request) {
      type(Request)
      min(Required)
    }
  }
  output (StockList) {
    on-empty{ // 에러처리 수정필요
      halt{
        dialog{
          template ("")
        }
      }
    }
  }
}
