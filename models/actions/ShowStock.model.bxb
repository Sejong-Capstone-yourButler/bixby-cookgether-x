action (ShowStock) {
  description (물어보는 메뉴의 재고를 화면에 보여준다.)
  type(Search)
  collect {
    input (menuname) {
      type (MenuName)
      min (Required)
    }
  }
  output (StockList) {
    on-empty{ // 에러처리 수정필요
      halt{
        dialog{
          template ("")
        }
      }
    }
  }
}