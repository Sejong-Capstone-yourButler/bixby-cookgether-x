action (SpeakLocation) {
  description (배달원의 위치를 말해준다.)
  // 배달원위치 % 말해줘
  type(Search)
  collect {
    input (riderKeyword) {
      type(RiderKeyword)
      min (Required)
    }
    input (request) {
      type(Request)
      min(Required) max (One)
    }
  }
  output (RiderDistance) {
     on-empty{
      halt{
        dialog{
          template ("아직 라이더가 배정되지 않았습니다.")
        }
      }
    }
  }
}