action (SpeakWaiting) {
  description (수용할 수 있는 최소인원과 해당메뉴의 남은 수량 말해준다.)
  // 추가수용가능인원수 % 보여줘
  type(Search)
  collect {
    input (waitingKeyword) {
      type (WaitingKeyword)
      min (Required)
    }
    input(request) {
      type (Request)
      min (Required)
    }
  }
  output (WaitingList) {
    on-empty{ // 에러처리 수정필요
      halt{
        dialog{
          template ("")
        }
      }
    }
  }
}